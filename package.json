{
  "name": "gemini-desktop-multi-user",
  "productName": "Gemini Desktop",
  "version": "1.0.2",
  "description": "Multi-User Gemini Desktop App with Session Isolation",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently -k \"tsc -p tsconfig.main.json -w\" \"vite\" \"wait-on tcp:5173 && electron .\"",
    "build": "tsc -p tsconfig.main.json && vite build",
    "dist": "electron-builder",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "playwright test"
  },
  "keywords": [
    "electron",
    "react",
    "gemini",
    "multi-user"
  ],
  "author": "Google Jules",
  "license": "MIT",
  "dependencies": {
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "electron": "^40.4.0",
    "electron-builder": "^26.7.0",
    "playwright": "^1.58.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wait-on": "^9.0.3"
  }
}
