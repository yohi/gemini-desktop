{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "dist/main",
    "module": "commonjs",
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "target": "ES6",
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": ["src/main/**/*"]
}
